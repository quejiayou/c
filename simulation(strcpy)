#include <stdio.h>
#include <assert.h>
char* my_strcpy(char* arr, const char* arr1)
{
	assert(arr != NULL);
	assert(arr1 != NULL);
	char* p = arr;
	while (*arr++ = *arr1++)
	{
		;
	}
	return p;
}
int main()
{
	char arr[20] = "***********";
	char arr1[] = "hello";
	printf("%s", my_strcpy(arr, arr1));
	return 0;
}
