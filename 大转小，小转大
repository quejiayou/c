#include <stdio.h>
int main()
{
	char arr[101];
	int len;
	int i, n, m;
	n = m = 0;
		gets(arr);
		len = strlen(arr);
		for (i = 0; i < len; i++)
		{
			if (arr[i] >= 'a' && arr[i] <= 'z')
			{
				arr[i] -= 32;
			}	
			else if (arr[i] >= 'A' && arr[i] <= 'Z')
			{
			    arr[i] += 32;
			}		
		}

		puts(arr);


	return 0;
}
